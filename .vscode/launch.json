      "args": [
        "4",
        "6",
        "1",
        "2",
        "10",
        "1",
        "3",
        "15",
        "1",
        "4",
        "20",
        "2",
        "3",
        "35",
        "2",
        "4",
        "25",
        "3",
        "4",
        "30"
